{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "./src/components/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/servers/*": [
        "./src/servers/*"
      ],
      "@/assets/*": [
        "./src/assets/*"
      ],
      "@/package": [
        "./package.json"
      ],
      "@/project": [
        "./project.config.json"
      ],
    },
    "rootDir": ".",
    "jsx": "react-jsx",
    "allowJs": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "./src",
    "./types"
  ],
  "compileOnSave": false
}